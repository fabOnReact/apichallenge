<h1>Comments#new</h1>
<p>Find me in app/views/comments/new.html.erb</p>
<%= form_for @comment do |f| %>
  <%= render 'shared/error_messages', object: @comment %>
  <%= f.label :message %>
  <%= f.text_field :message %>
  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit %>
<% end %>
